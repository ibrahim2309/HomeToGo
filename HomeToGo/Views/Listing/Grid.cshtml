@model IEnumerable<Listing>

    <h1>List of Listings Items (@ViewBag.CurrentViewName View)</h1>
    <a href="/Listing/Table">Table View</a>

    <div class="row row-cols-1 row-cols-md-3 g-4">
    @foreach (var listing in Model)
{
    <div class="col">
        <div>
        <img src="@listing.ImageUrl" class="card-img-top" alt="@listing.Title">
        <div>
        <div class="d-flex justify-content-between mt-2">
        <h5 class="text-start">
        @listing.Title
        </h5>
        <h5 class="text-nowrap">
        @listing.Price.ToString("0.00 Nok")
        </h5>
        </div>
        </div>
        </div>
        </div>
}
</div>