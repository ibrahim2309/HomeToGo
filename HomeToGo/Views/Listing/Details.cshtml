@model Listing;

<div class="row gx-5">
    <img alt="@Model.Title" src="@Model.ImageUrl" class="img-fluid col-5" />
    <div class="col-7">
        <h5>@Model.Description</h5>
        <h3 class="pull-right">@Model.Price.ToString("0.00 NOK")</h3>
        <p><a href="/Listing/Table">Back to Table View</a></p>
        <p><a href="/Listing/Grid">Back to Grid View</a></p>
    </div>
</div>